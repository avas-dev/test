<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="notificationsNomenuPostLoginVM" type="com.avassapplication.app.modules.notificationsnomenupostlogin.data.viewmodel.NotificationsNomenuPostLoginVM"/>
    </data>
    <LinearLayout android:gravity="center_horizontal"
        android:id="@+id/linearNotifications"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylegray_50">
        <FrameLayout android:gravity="start"
            android:id="@+id/frameStackimg"
            android:layout_height="@dimen/_398pxv" android:layout_width="match_parent">
            <ImageView android:id="@+id/imageImg"
                android:layout_gravity="start"
                android:layout_height="@dimen/_398pxh"
                android:layout_width="match_parent"
                android:scaleType="fitXY"
                imageRes="@{@drawable/img_img_398x375}"
                tools:ignore="ContentDescription" tools:src="@drawable/img_img_398x375"/>
            <TextView android:ellipsize="end" android:id="@+id/txtSKIP"
                android:layout_gravity="top|end"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_19pxv"
                android:layout_marginEnd="@dimen/_43pxh"
                android:layout_marginStart="@dimen/_43pxh"
                android:layout_marginTop="@dimen/_19pxv"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@{notificationsNomenuPostLoginVM.notificationsNomenuPostLoginModel.txtSKIP}"
                style="@style/txtPoppinsmedium16" tools:text="@string/lbl_skip2"/>
        </FrameLayout>
        <ImageView android:id="@+id/imageBell"
            android:layout_gravity="start"
            android:layout_height="@dimen/_45pxh"
            android:layout_marginEnd="@dimen/_37pxh"
            android:layout_marginStart="@dimen/_37pxh"
            android:layout_marginTop="@dimen/_44pxv"
            android:layout_width="@dimen/_41pxh"
            android:scaleType="fitXY" android:src="@drawable/img_bell"
            tools:ignore="ContentDescription" tools:src="@drawable/img_bell"/>
        <TextView android:ellipsize="end" android:gravity="start"
            android:id="@+id/txtTitle"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_31pxh"
            android:layout_marginStart="@dimen/_31pxh"
            android:layout_marginTop="@dimen/_19pxv"
            android:layout_width="match_parent"
            android:lineSpacingExtra="@dimen/_12pxh"
            android:singleLine="true"
            android:text="@{notificationsNomenuPostLoginVM.notificationsNomenuPostLoginModel.txtTitle}"
            style="@style/txtPoppinsbold24_1" tools:text="@string/msg_get_daily_affir"/>
        <TextView android:gravity="start"
            android:id="@+id/txtDescription"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_31pxh"
            android:layout_marginStart="@dimen/_31pxh"
            android:layout_marginTop="@dimen/_30pxv"
            android:layout_width="match_parent"
            android:text="@{notificationsNomenuPostLoginVM.notificationsNomenuPostLoginModel.txtDescription}"
            style="@style/txtNunitomedium15" tools:text="@string/msg_enable_notifica"/>
        <FrameLayout android:id="@+id/frameStackvectorthree"
            android:layout_gravity="center_horizontal"
            android:layout_height="@dimen/_54pxv"
            android:layout_marginEnd="@dimen/_16pxh"
            android:layout_marginStart="@dimen/_16pxh"
            android:layout_marginTop="@dimen/_29pxv" android:layout_width="@dimen/_343pxh">
            <LinearLayout android:id="@+id/linearEnabled"
                android:layout_gravity="start"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_14pxv"
                android:layout_marginEnd="@dimen/_56pxh"
                android:layout_marginStart="@dimen/_56pxh"
                android:layout_marginTop="@dimen/_14pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageButton
                    android:background="@drawable/rectangle_bg_green_500_radius_11"
                    android:id="@+id/btnVectorThree"
                    android:layout_height="@dimen/_22pxh"
                    android:layout_marginTop="@dimen/_2pxv"
                    android:layout_width="@dimen/_22pxh"
                    android:paddingHorizontal="@dimen/_5pxh"
                    android:paddingVertical="@dimen/_5pxv"
                    android:scaleType="fitCenter"
                    android:src="@drawable/img_vector3" tools:ignore="ContentDescription"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtEnabledNotific"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_marginStart="@dimen/_14pxh"
                    android:layout_weight="0.89"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@{notificationsNomenuPostLoginVM.notificationsNomenuPostLoginModel.txtEnabledNotific}"
                    style="@style/txtPoppinsbold16" tools:text="@string/msg_enabled_notific"/>
            </LinearLayout>
            <androidx.appcompat.widget.AppCompatButton
                android:ellipsize="end" android:gravity="center"
                android:id="@+id/btnEnableNotifications"
                android:layout_gravity="start"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:paddingBottom="@dimen/_15pxv"
                android:paddingEnd="@dimen/_30pxh"
                android:paddingStart="@dimen/_30pxh"
                android:paddingTop="@dimen/_15pxv"
                android:singleLine="true"
                android:text="@string/msg_enable_notifica2"
                android:textAllCaps="false"
                style="@style/btnSolidRounded_3" tools:text="@string/msg_enable_notifica2"/>
        </FrameLayout>
        <androidx.appcompat.widget.AppCompatButton
            android:ellipsize="end" android:gravity="center"
            android:id="@+id/btnContinue"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_20pxv"
            android:layout_marginEnd="@dimen/_16pxh"
            android:layout_marginStart="@dimen/_16pxh"
            android:layout_marginTop="@dimen/_18pxv"
            android:layout_width="match_parent"
            android:paddingBottom="@dimen/_15pxv"
            android:paddingEnd="@dimen/_30pxh"
            android:paddingStart="@dimen/_30pxh"
            android:paddingTop="@dimen/_15pxv" android:singleLine="true"
            android:text="@string/lbl_continue"
            android:textAllCaps="false" style="@style/btnSolidRounded_4" tools:text="@string/lbl_continue"/>
    </LinearLayout>
</layout>
