<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="soundListStateDefaultSoundVM" type="com.avassapplication.app.modules.soundliststatedefaultsound.data.viewmodel.SoundListStateDefaultSoundVM"/>
    </data>
    <LinearLayout android:gravity="center_horizontal"
        android:id="@+id/linearSoundliststa"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylewhite_A700">
        <FrameLayout android:gravity="start"
            android:id="@+id/frameTopIMGBack"
            android:layout_height="@dimen/_276pxv" android:layout_width="match_parent">
            <ImageView android:id="@+id/imageMeditationimag"
                android:layout_gravity="start"
                android:layout_height="@dimen/_276pxh"
                android:layout_width="match_parent"
                android:scaleType="fitXY"
                imageRes="@{@drawable/img_meditationimag_276x375}"
                tools:ignore="ContentDescription" tools:src="@drawable/img_meditationimag_276x375"/>
            <LinearLayout android:id="@+id/linearColumncategory"
                android:layout_gravity="top|start"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_10pxv"
                android:layout_width="match_parent" android:orientation="vertical">
                <FrameLayout android:gravity="start"
                    android:id="@+id/frameStackcategory"
                    android:layout_height="@dimen/_105pxv" android:layout_width="match_parent">
                    <LinearLayout android:id="@+id/linearUpgradebutton"
                        android:layout_gravity="bottom|end"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_8pxv"
                        android:layout_marginEnd="@dimen/_19pxh"
                        android:layout_marginStart="@dimen/_19pxh"
                        android:layout_marginTop="@dimen/_10pxv"
                        android:layout_width="wrap_content"
                        android:orientation="vertical" style="@style/groupStyledeep_purple_500cornerRadius">
                        <TextView android:ellipsize="end"
                            android:gravity="center_horizontal"
                            android:id="@+id/txtCategory"
                            android:layout_height="match_parent"
                            android:layout_marginBottom="@dimen/_8pxv"
                            android:layout_marginEnd="@dimen/_11pxh"
                            android:layout_marginStart="@dimen/_12pxh"
                            android:layout_marginTop="@dimen/_7pxv"
                            android:layout_width="match_parent"
                            android:singleLine="true"
                            android:text="@{soundListStateDefaultSoundVM.soundListStateDefaultSoundModel.txtCategory}"
                            style="@style/txtNunitobold13" tools:text="@string/lbl_upgrade"/>
                    </LinearLayout>
                    <LinearLayout android:id="@+id/linearTopwhenscroll"
                        android:layout_gravity="start"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent"
                        android:orientation="horizontal" style="@style/groupStylewhite_A700">
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtApptitle"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_23pxv"
                            android:layout_marginStart="@dimen/_149pxh"
                            android:layout_marginTop="@dimen/_51pxv"
                            android:layout_weight="0.45"
                            android:layout_width="0dp"
                            android:singleLine="true"
                            android:text="@{soundListStateDefaultSoundVM.soundListStateDefaultSoundModel.txtApptitle}"
                            style="@style/txtPoppinsbold20" tools:text="@string/lbl_sounds"/>
                        <androidx.appcompat.widget.AppCompatButton
                            android:ellipsize="end"
                            android:gravity="center"
                            android:id="@+id/btnUpgrade"
                            android:layout_height="@dimen/_34pxv"
                            android:layout_marginBottom="@dimen/_22pxv"
                            android:layout_marginEnd="@dimen/_16pxh"
                            android:layout_marginStart="@dimen/_38pxh"
                            android:layout_marginTop="@dimen/_49pxv"
                            android:layout_width="@dimen/_94pxh"
                            android:paddingBottom="@dimen/_8pxv"
                            android:paddingEnd="@dimen/_11pxh"
                            android:paddingStart="@dimen/_12pxh"
                            android:paddingTop="@dimen/_8pxv"
                            android:singleLine="true"
                            android:text="@string/lbl_upgrade"
                            android:textAllCaps="false"
                            style="@style/btnSolidRounded" tools:text="@string/lbl_upgrade"/>
                    </LinearLayout>
                </FrameLayout>
                <TextView android:id="@+id/txtMeditationtitl"
                    android:layout_gravity="start"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_23pxh"
                    android:layout_marginStart="@dimen/_23pxh"
                    android:layout_marginTop="@dimen/_66pxv"
                    android:layout_width="wrap_content"
                    android:text="@{soundListStateDefaultSoundVM.soundListStateDefaultSoundModel.txtMeditationtitl}"
                    style="@style/txtPoppinssemibold30" tools:text="@string/msg_spring_meditat"/>
            </LinearLayout>
        </FrameLayout>
        <TextView android:gravity="start"
            android:id="@+id/txtDescription"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_21pxh"
            android:layout_marginStart="@dimen/_21pxh"
            android:layout_marginTop="@dimen/_14pxv"
            android:layout_width="match_parent"
            android:text="@{soundListStateDefaultSoundVM.soundListStateDefaultSoundModel.txtDescription}"
            style="@style/txtPoppinsregular14" tools:text="@string/msg_short_meditatio"/>
        <LinearLayout android:id="@+id/linearAudiocontainer"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_375pxv"
            android:layout_marginEnd="@dimen/_12pxh"
            android:layout_marginStart="@dimen/_13pxh"
            android:layout_marginTop="@dimen/_28pxv"
            android:layout_width="match_parent"
            android:orientation="horizontal" style="@style/groupStylewhite_4">
            <LinearLayout android:id="@+id/linearRowplay"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_20pxv"
                android:layout_marginStart="@dimen/_24pxh"
                android:layout_marginTop="@dimen/_20pxv"
                android:layout_weight="0.88" android:layout_width="0dp" android:orientation="horizontal">
                <FrameLayout android:id="@+id/frameStackplay"
                    android:layout_height="@dimen/_35pxv" android:layout_width="@dimen/_35pxh">
                    <ImageView android:id="@+id/imagePlay"
                        android:layout_gravity="start"
                        android:layout_height="@dimen/_35pxh"
                        android:layout_width="@dimen/_35pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_play"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_play"/>
                    <ImageView android:id="@+id/imagePlayactionpl"
                        android:layout_gravity="start"
                        android:layout_height="@dimen/_35pxh"
                        android:layout_width="@dimen/_35pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_playactionpl_indigo_a700"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_playactionpl_indigo_a700"/>
                </FrameLayout>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtLanguage"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_7pxv"
                    android:layout_marginStart="@dimen/_8pxh"
                    android:layout_marginTop="@dimen/_4pxv"
                    android:layout_weight="0.84"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@{soundListStateDefaultSoundVM.soundListStateDefaultSoundModel.txtLanguage}"
                    style="@style/txtPoppinssemibold15" tools:text="@string/msg_name_of_the_med"/>
            </LinearLayout>
            <TextView android:ellipsize="end" android:id="@+id/txtTime"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_26pxv"
                android:layout_marginEnd="@dimen/_17pxh"
                android:layout_marginStart="@dimen/_53pxh"
                android:layout_marginTop="@dimen/_27pxv"
                android:layout_weight="0.12" android:layout_width="0dp"
                android:singleLine="true"
                android:text="@{soundListStateDefaultSoundVM.soundListStateDefaultSoundModel.txtTime}"
                style="@style/txtPoppinssemibold14_1" tools:text="@string/lbl_0_00"/>
        </LinearLayout>
    </LinearLayout>
</layout>
