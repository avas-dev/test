<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="intro2NomenuVM" type="com.avassapplication.app.modules.intro2nomenu.data.viewmodel.Intro2NomenuVM"/>
    </data>
    <LinearLayout android:gravity="center_horizontal"
        android:id="@+id/linearIntro2nomenu"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStyle_2">
        <androidx.appcompat.widget.Toolbar
            android:gravity="center_horizontal"
            android:id="@+id/toolbarToolbar"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_16pxh"
            android:layout_marginStart="@dimen/_16pxh"
            android:layout_marginTop="@dimen/_64pxv"
            android:layout_width="match_parent" app:contentInsetStart="0dp">
            <LinearLayout android:id="@+id/linearTopContainer"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageButton
                    android:background="@drawable/rectangle_bg_white_a700_radius_8"
                    android:id="@+id/btnArrowleft"
                    android:layout_height="@dimen/_40pxh"
                    android:layout_width="@dimen/_40pxh"
                    android:paddingHorizontal="@dimen/_14pxh"
                    android:paddingVertical="@dimen/_14pxv"
                    android:scaleType="fitCenter"
                    android:src="@drawable/img_arrowleft" tools:ignore="ContentDescription"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtSkipactioncl"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_15pxv"
                    android:layout_marginStart="@dimen/_268pxh"
                    android:layout_marginTop="@dimen/_2pxv"
                    android:layout_weight="0.47"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@{intro2NomenuVM.intro2NomenuModel.txtSkipactioncl}"
                    style="@style/txtManropeextrabold16_1" tools:text="@string/lbl_skip"/>
            </LinearLayout>
        </androidx.appcompat.widget.Toolbar>
        <FrameLayout android:id="@+id/frameContainercont"
            android:layout_gravity="center_horizontal"
            android:layout_height="@dimen/_354pxv"
            android:layout_marginEnd="@dimen/_16pxh"
            android:layout_marginStart="@dimen/_16pxh"
            android:layout_marginTop="@dimen/_354pxv" android:layout_width="@dimen/_343pxh">
            <FrameLayout android:id="@+id/frameCTA"
                android:layout_gravity="bottom|start"
                android:layout_height="@dimen/_107pxv"
                android:layout_marginTop="@dimen/_10pxv" android:layout_width="@dimen/_343pxh">
                <View
                    android:background="@drawable/rectangle_bg_indigo_a700_radius_27"
                    android:id="@+id/viewBg"
                    android:layout_gravity="top|start"
                    android:layout_height="@dimen/_54pxv"
                    android:layout_marginBottom="@dimen/_10pxv"
                    android:layout_marginTop="@dimen/_5pxv" android:layout_width="@dimen/_343pxh"/>
                <LinearLayout android:id="@+id/linearNaprej"
                    android:layout_gravity="center"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_40pxh"
                    android:layout_marginStart="@dimen/_40pxh"
                    android:layout_width="wrap_content"
                    android:orientation="vertical" style="@style/groupStylecornerRadius_1">
                    <TextView android:ellipsize="end"
                        android:gravity="center_horizontal"
                        android:id="@+id/txtNaprej"
                        android:layout_height="match_parent"
                        android:layout_marginBottom="@dimen/_62pxv"
                        android:layout_marginEnd="@dimen/_30pxh"
                        android:layout_marginStart="@dimen/_30pxh"
                        android:layout_marginTop="@dimen/_21pxv"
                        android:layout_width="match_parent"
                        android:singleLine="true"
                        android:text="@{intro2NomenuVM.intro2NomenuModel.txtNaprej}"
                        style="@style/txtPoppinsextrabold16" tools:text="@string/lbl_naprej"/>
                </LinearLayout>
            </FrameLayout>
            <TextView android:gravity="top|start"
                android:id="@+id/txtIntroOne"
                android:layout_gravity="start"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_102pxv"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_marginTop="@dimen/_102pxv"
                android:layout_width="match_parent"
                android:text="@{intro2NomenuVM.intro2NomenuModel.txtIntroOne}"
                style="@style/txtPoppinsmedium31" tools:text="@string/msg_obvladujte_stre"/>

            <com.asksira.loopingviewpager.indicator.CustomShapePagerIndicator
                android:id="@+id/indicatorGroupTwentyThree"
                android:layout_gravity="top|start"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_83pxv"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_marginStart="@dimen/_6pxh"
                android:layout_marginTop="@dimen/_83pxv"
                android:layout_width="wrap_content"
                app:drawable_normal="@drawable/indicator_unselected_bg_white_a700_66_radius_8"
                app:drawable_selected="@drawable/indicator_selected_bg_indigo_a700_radius_8"
                app:indicator_size="@dimen/_32pxh" app:indicator_spacing="@dimen/_8pxh"/>
            <ImageView android:id="@+id/imageLOTUS"
                android:layout_gravity="top|start"
                android:layout_height="@dimen/_64pxh"
                android:layout_marginBottom="@dimen/_10pxv"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_marginStart="@dimen/_6pxh"
                android:layout_width="@dimen/_100pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_lotus"
                tools:ignore="ContentDescription" tools:src="@drawable/img_lotus"/>
            <LinearLayout android:id="@+id/linearContainercont"
                android:layout_gravity="start"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="vertical">
                <ImageView android:id="@+id/imageLOTUSOne"
                    android:layout_gravity="start"
                    android:layout_height="@dimen/_64pxh"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_marginStart="@dimen/_6pxh"
                    android:layout_width="@dimen/_100pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_lotus"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_lotus"/>
                <LinearLayout android:id="@+id/linearColumnthirteen"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_18pxv"
                    android:layout_width="match_parent" android:orientation="vertical">
                    <LinearLayout android:gravity="start"
                        android:id="@+id/linearRowthirteen"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_10pxh"
                        android:layout_marginStart="@dimen/_6pxh"
                        android:layout_width="wrap_content" android:orientation="horizontal">
                        <View android:background="@color/white_A700_66"
                            android:id="@+id/viewThirteen"
                            android:layout_height="@dimen/_4pxv" android:layout_width="@dimen/_32pxh"/>
                        <View android:background="@color/indigo_A700"
                            android:id="@+id/viewTwentyThree"
                            android:layout_height="@dimen/_4pxv"
                            android:layout_marginStart="@dimen/_8pxh" android:layout_width="@dimen/_32pxh"/>
                        <View android:background="@color/white_A700_66"
                            android:id="@+id/viewThirtyThree"
                            android:layout_height="@dimen/_4pxv"
                            android:layout_marginStart="@dimen/_8pxh" android:layout_width="@dimen/_32pxh"/>
                    </LinearLayout>
                    <FrameLayout android:id="@+id/frameStackbgOne"
                        android:layout_gravity="start"
                        android:layout_height="@dimen/_247pxv"
                        android:layout_marginTop="@dimen/_19pxv" android:layout_width="@dimen/_343pxh">
                        <FrameLayout android:id="@+id/frameCTA1"
                            android:layout_gravity="bottom|start"
                            android:layout_height="@dimen/_107pxv"
                            android:layout_marginTop="@dimen/_10pxv" android:layout_width="@dimen/_343pxh">
                            <View
                                android:background="@drawable/rectangle_bg_indigo_a700_radius_27"
                                android:id="@+id/viewBgOne"
                                android:layout_gravity="top|start"
                                android:layout_height="@dimen/_54pxv"
                                android:layout_marginBottom="@dimen/_10pxv"
                                android:layout_marginTop="@dimen/_5pxv" android:layout_width="@dimen/_343pxh"/>
                            <LinearLayout
                                android:id="@+id/linearNaprej1"
                                android:layout_gravity="center"
                                android:layout_height="wrap_content"
                                android:layout_marginEnd="@dimen/_40pxh"
                                android:layout_marginStart="@dimen/_40pxh"
                                android:layout_width="wrap_content"
                                android:orientation="vertical" style="@style/groupStylecornerRadius_1">
                                <TextView android:ellipsize="end"
                                    android:gravity="center_horizontal"
                                    android:id="@+id/txtNaprejOne"
                                    android:layout_height="match_parent"
                                    android:layout_marginBottom="@dimen/_62pxv"
                                    android:layout_marginEnd="@dimen/_30pxh"
                                    android:layout_marginStart="@dimen/_30pxh"
                                    android:layout_marginTop="@dimen/_21pxv"
                                    android:layout_width="match_parent"
                                    android:singleLine="true"
                                    android:text="@{intro2NomenuVM.intro2NomenuModel.txtNaprejOne}"
                                    style="@style/txtPoppinsextrabold16" tools:text="@string/lbl_naprej"/>
                            </LinearLayout>
                        </FrameLayout>
                        <TextView android:gravity="top|start"
                            android:id="@+id/txtIntroOneOne"
                            android:layout_gravity="top|start"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_10pxv"
                            android:layout_marginEnd="@dimen/_10pxh"
                            android:layout_width="match_parent"
                            android:text="@{intro2NomenuVM.intro2NomenuModel.txtIntroOneOne}"
                            style="@style/txtPoppinsmedium31" tools:text="@string/msg_obvladujte_stre"/>
                    </FrameLayout>
                </LinearLayout>
            </LinearLayout>
        </FrameLayout>
    </LinearLayout>
</layout>
